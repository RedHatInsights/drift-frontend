{
  "name": "drift-frontend",
  "version": "1.1.0",
  "private": false,
  "dependencies": {
    "@babel/runtime": "^7.11.0",
    "@patternfly/patternfly": "^4.23.3",
    "@patternfly/react-core": "4.47.0",
    "@patternfly/react-icons": "^4.7.16",
    "@patternfly/react-table": "^4.19.24",
    "@redhat-cloud-services/frontend-components": "2.5.1",
    "@redhat-cloud-services/frontend-components-notifications": "^2.1.2",
    "@redhat-cloud-services/frontend-components-utilities": "2.2.8",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "jiff": "^0.7.3",
    "prop-types": "^15.7.2",
    "react": "16.11.0",
    "react-dom": "16.9.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6"
  },
  "sassIncludes": {
    "patternfly": "node_modules/patternfly/dist/sass",
    "bootstrap": "node_modules/patternfly/node_modules/bootstrap-sass/assets/stylesheets",
    "fontAwesome": "node_modules/patternfly/node_modules/font-awesome-sass/assets/stylesheets"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js",
      "!<rootDir>/src/**/index.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.+fixtures.+"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-flow": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-dual-import": "^1.2.1",
    "babel-plugin-lodash": "^3.3.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "cross-fetch": "^2.2.3",
    "css-loader": "^0.28.11",
    "d3": "^5.16.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.10.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-react": "^7.20.5",
    "file-loader": "^1.1.11",
    "git-revision-webpack-plugin": "^3.0.6",
    "glob": "^7.1.6",
    "html-replace-webpack-plugin": "^2.5.6",
    "html-webpack-plugin": "^3.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "lodash-webpack-plugin": "^0.11.5",
    "material-ui": "^0.20.2",
    "mini-css-extract-plugin": "^0.4.5",
    "moment": "^2.27.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "redux-mock-store": "^1.5.4",
    "redux-promise-middleware": "^5.1.1",
    "regenerator-runtime": "^0.12.1",
    "sass-loader": "^7.3.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.21.0",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0",
    "topojson": "^3.0.2",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-visualizer-plugin": "^0.1.11",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "scripts": {
    "build": "webpack --config config/prod.webpack.config.js",
    "test": "jest --verbose",
    "test-watch": "jest --verbose --watch",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "prod": "NODE_ENV=production webpack-dev-server --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_ENV=development webpack-dev-server --config config/dev.webpack.config.js",
    "travis:build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "travis:verify": "npm-run-all travis:build lint test",
    "verify": "npm-run-all build lint test",
    "nightly": "npm run travis:verify",
    "update-snapshot": "jest -u"
  },
  "insights": {
    "appname": "drift"
  }
}
